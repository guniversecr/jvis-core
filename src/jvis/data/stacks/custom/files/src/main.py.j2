from http.server import HTTPServer

from src.handlers import ItemHandler

HOST = "0.0.0.0"
PORT = 8000


def main() -> None:
    server = HTTPServer((HOST, PORT), ItemHandler)
    print(f"{{ project_name }} running on http://{HOST}:{PORT}")
    server.serve_forever()


if __name__ == "__main__":
    main()
